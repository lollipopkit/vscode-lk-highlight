{
    "name": "vscode-lk-highlight",
    "displayName": "lang lk highlighting",
    "description": "Provides highlighting (and nothing else!) for lang lk",
    "keywords": [
        "lk",
        "highlight"
    ],
    "version": "0.0.3",
    "repository": {
        "type": "git",
        "url": "https://github.com/lollipopkit/vscode-lk-highlight"
    },
    "bugs": {
        "url": "https://github.com/lollipopkit/vscode-lk-highlight/issues"
    },
    "license": "MIT",
    "publisher": "lollipopkit",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "lk",
                "aliases": [
                    "lk"
                ],
                "extensions": [
                    ".lk"
                ],
                "firstLine": "^#!/.*\\blk\\b",
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "lk",
                "scopeName": "source.lk",
                "path": "./syntaxes/lk.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "@vscode/vsce": "^2.19.0"
    }
}
